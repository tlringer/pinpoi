# Flow policy
ACG("location") -> DATABASE, DISPLAY, BUNDLE, INTENT, SHARED_PREFERENCES, WRITE_LOGS, INTERNET, FILESYSTEM

# Non-ACG permission model composition
FILESYSTEM -> DATABASE, DISPLAY, BUNDLE, INTENT, SHARED_PREFERENCES, WRITE_LOGS, INTERNET, FILESYSTEM
DATABASE -> DATABASE, DISPLAY, BUNDLE, INTENT, SHARED_PREFERENCES, WRITE_LOGS, INTERNET, FILESYSTEM
USER_INPUT -> DATABASE, DISPLAY, BUNDLE, INTENT, SHARED_PREFERENCES, WRITE_LOGS, INTERNET, FILESYSTEM
INTENT -> DATABASE, DISPLAY, SHARED_PREFERENCES, BUNDLE, INTENT, WRITE_LOGS, INTERNET, FILESYSTEM
READ_TIME -> DATABASE, DISPLAY, BUNDLE, INTENT, SHARED_PREFERENCES, WRITE_LOGS, INTERNET, FILESYSTEM
BUNDLE -> DATABASE, DISPLAY, SHARED_PREFERENCES, INTENT, BUNDLE, WRITE_LOGS, INTERNET, FILESYSTEM
SHARED_PREFERENCES -> DATABASE, DISPLAY, INTENT, BUNDLE, SHARED_PREFERENCES, WRITE_LOGS, INTERNET, FILESYSTEM

